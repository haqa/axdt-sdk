<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="org.axdt.core.compiler.axdtCompiler">
      <compiler
            class="org.axdt.flex4.compiler.Flex4Compiler"
            id="org.axdt.flex4.compiler"
            language="As3"
            name="%flexCompilerName">
      </compiler>
   </extension>
   <extension
         point="org.axdt.core.compiler.axdtDebugger">
      <debugger
            class="org.axdt.flex4.debugger.Flex4Debugger"
            id="org.axdt.flex4.debugger"
            language="As3"
            name="%flexDebuggerName">
      </debugger>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
         <category name="Axdt" id="org.axdt.category"></category>
		<wizard name="Flex config File" icon="icons/flexconfig_file.gif"
			category="org.axdt.category" class="org.axdt.flex4.wizards.NewFlexConfigFileWizard"
			id="org.axdt.flex4.wizards.newFlexConfigWizard">
		</wizard>
   </extension>
	<extension point="org.eclipse.wst.xml.core.catalogContributions">
		<catalogContribution>
			<public publicId="http://www.adobe.com/2006/flex-config"
				uri="src/main/xsd/flex-config.xsd" />
		</catalogContribution>
	</extension>
	<extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.axdt.core.ui.perspective">
         <newWizardShortcut
               id="org.axdt.flex4.wizards.newFlexConfigWizard">
         </newWizardShortcut>
      </perspectiveExtension>
    </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.axdt.core.ui.preferences"
            class="org.axdt.flex4.preferences.Flex4PreferencePage"
            id="org.axdt.flex4.preferences"
            name="%flexPref"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.axdt.flex4.preferences.Flex4Preferences"/>
   </extension>
</fragment>
