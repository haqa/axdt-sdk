<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.axdt.flex4.feature"
      label="%featureName"
      version="0.0.82.qualifier"
      provider-name="%providerName">

   <description url="%descriptionUrl">
      %description
   </description>

   <copyright url="%copyrightUrl">
      %copyright
   </copyright>

   <license url="%licenseUrl">
      %license
   </license>

   <url>
      <update label="%updateName" url="http://svn.codespot.com/a/eclipselabs.org/axdt/trunk/site/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.variables"/>
      <import plugin="org.apache.ant"/>
      <import plugin="org.axdt.flex4.sdk" version="4.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ant.core"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.console"/>
      <import plugin="org.axdt.core.compiler"/>
      <import plugin="org.eclipse.wst.xml.core" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.axdt.as3.debug"/>
      <import plugin="org.axdt.core.ui"/>
      <import plugin="org.apache.log4j"/>
   </requires>

   <plugin
         id="org.axdt.flex4.antsupport"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.axdt.flex4.integration"
         download-size="0"
         install-size="0"
         version="0.0.0"
         fragment="true"
         unpack="false"/>

   <plugin
         id="org.axdt.flex4.sdk"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

   <plugin
         id="org.axdt.flex4.config"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
